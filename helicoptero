import javax.Swing.JoptionPane;
public class helicoptero extends aereo
{
   int pies;
   pubic helicoptero(String tipo, String capacidad, String color, String velocidad, int precio ){
        super (tipo, capacidad, color, velocidad, precio);
        this.pies =pies;
}


 @Override
 public void verDatos(){
    JoptionPane.ShowMessageDiaolog(null, "tipo: " + tipo + "\ncapacidad: " + capacidad + "\ncolor: "+ color + "\nvelocidad: " + velocidad + "Datos del avion");
}
}
